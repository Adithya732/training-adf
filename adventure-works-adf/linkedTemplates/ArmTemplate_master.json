{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adventure-works-adf"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"LinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkedService'"
		},
		"SQLdb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLdb'"
		},
		"demo_dl_sql_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'demo_dl_sql'"
		},
		"Keysql_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://sqltraining.vault.azure.net/"
		},
		"demo_dl_sql_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adventurewokltdl.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adventure-works-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"LinkedService_connectionString": {
						"value": "[parameters('LinkedService_connectionString')]"
					},
					"SQLdb_connectionString": {
						"value": "[parameters('SQLdb_connectionString')]"
					},
					"demo_dl_sql_accountKey": {
						"value": "[parameters('demo_dl_sql_accountKey')]"
					},
					"Keysql_properties_typeProperties_baseUrl": {
						"value": "[parameters('Keysql_properties_typeProperties_baseUrl')]"
					},
					"demo_dl_sql_properties_typeProperties_url": {
						"value": "[parameters('demo_dl_sql_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}